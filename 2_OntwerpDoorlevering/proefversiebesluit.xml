	<tekst:RegelingCompact xmlns:tekst="https://standaarden.overheid.nl/stop/imop/tekst/">
	  <tekst:RegelingOpschrift eId="longTitle" wId="longTitle">
	    <tekst:Al>Vaststelling van "Voorbeeldregels Hello DSO" van de gemeente Hello</tekst:Al>
	  </tekst:RegelingOpschrift>
	  <tekst:Lichaam eId="body" wId="body">
	    <tekst:Hoofdstuk eId="chp_0" wId="gm0160_v1__chp_0">
	      <tekst:Kop>
		<tekst:Label>Hoofdstuk</tekst:Label>
		<tekst:Nummer>0</tekst:Nummer>
		<tekst:Opschrift>Over dit omgevingsplan</tekst:Opschrift>
	      </tekst:Kop>
	      <tekst:Artikel eId="chp_0__art_0.1" wId="gm0160_v1__chp_0__art_0.1">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>0.1</tekst:Nummer>
		  <tekst:Opschrift>Toepassingsgebied</tekst:Opschrift>
		</tekst:Kop>
		<tekst:Inhoud>
		  <tekst:Al><tekst:IntRef ref="cmp_I">Bijlage I</tekst:IntRef> bij dit omgevingsplan bevat begripsbepalingen voor de toepassing van dit omgevingsplan.</tekst:Al>
		</tekst:Inhoud>
	      </tekst:Artikel>
	    </tekst:Hoofdstuk>
	    <tekst:Hoofdstuk eId="chp_1" wId="gm0160_v1__chp_1">
	      <tekst:Kop>
		<tekst:Label><tekst:VerwijderdeTekst>HOOFDSTUK</tekst:VerwijderdeTekst><tekst:NieuweTekst>Hoofdstuk</tekst:NieuweTekst></tekst:Label>
		<tekst:Nummer>1</tekst:Nummer>
		<tekst:Opschrift><tekst:VerwijderdeTekst>ALGEMENE BEPALINGEN</tekst:VerwijderdeTekst><tekst:NieuweTekst>Algemene
                                 bepalingen</tekst:NieuweTekst></tekst:Opschrift>
	      </tekst:Kop>
	      <tekst:Artikel eId="chp_1__art_1" wId="gm0160_v1__chp_1__art_1">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>1.1</tekst:Nummer>
		  <tekst:Opschrift>Begripsbepalingen</tekst:Opschrift>
		</tekst:Kop>
		<tekst:Lid eId="chp_1__art_1__para_1" wId="gm0160_v1__chp_1__art_1__para_1">
		  <tekst:LidNummer>1.</tekst:LidNummer>
		  <tekst:Inhoud>
		    <tekst:Al>Begripsbepalingen die, op de dag van de inwerkingtreding van de Omgevingswet, zijn
                                         opgenomen in de bijlage bij de Omgevingswet en in bijlage I bij het Besluit activiteiten
                                         leefomgeving, bijlage I bij het Besluit bouwwerken leefomgeving, bijlage I bij het
                                         Besluit kwaliteit leefomgeving, bijlage I bij het Omgevingsbesluit en bijlage I bij de
                                         Omgevingsregeling, zijn van toepassing op dit omgevingsplan.</tekst:Al>
		  </tekst:Inhoud>
		</tekst:Lid>
		<tekst:Lid eId="chp_1__art_1.1__para_2" wId="gm0160_v1__chp_1__art_1.1__para_2">
		  <tekst:LidNummer>2.</tekst:LidNummer>
		  <tekst:Inhoud>
		    <tekst:Al>Vanaf de datum van inwerkingtreding maakt <tekst:IntIoRef eId="chp_1_art_1.1__ref_1" ref="gm0160_v2__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1" wId="gm0160_v1__chp_1_art_1.1__ref_1"><tekst:VerwijderdeTekst>Noordereilanden</tekst:VerwijderdeTekst><tekst:NieuweTekst>Noorderburen</tekst:NieuweTekst></tekst:IntIoRef>
                                         <tekst:NieuweTekst>(voorheen bekend als Noordereilanden) </tekst:NieuweTekst>deel uit van het
                                         ambtsgebied.</tekst:Al>
		  </tekst:Inhoud>
		</tekst:Lid>
		<tekst:Lid eId="chp_1__art_1.1__para_3" wId="gm0160_v1__chp_1__art_1.1__para_3">
		  <tekst:LidNummer>3.</tekst:LidNummer>
		  <tekst:Inhoud>
		    <tekst:Al wijzigactie="verwijder">Tot de kustgebieden in dit omgevingsplan moeten worden
                                         gerekend: <tekst:IntIoRef eId="chp_1_art_1.1__ref_2_inst2" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_2__ref_o_1" wId="gm0160_v1__chp_1_art_1.1__ref_2_inst2">Zuiderburen</tekst:IntIoRef> en <tekst:IntIoRef eId="chp_1_art1.1__ref_3_inst2" ref="gm0160_v2__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1" wId="gm0160_v1__chp_1_art1.1__ref_3_inst2">Noordereilanden</tekst:IntIoRef></tekst:Al>
		    <tekst:Al wijzigactie="voegtoe">Tot de stiltegebieden in dit omgevingsplan moeten worden
                                         gerekend: <tekst:IntIoRef eId="chp_1_art_1.1__ref_2" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_2__ref_o_1" wId="gm0160_v1__chp_1_art_1.1__ref_2">Zuiderburen</tekst:IntIoRef> (inclusief het zuidboveneiland) en <tekst:IntIoRef eId="chp_1__art_1.1__ref_3" ref="gm0160_v2__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1" wId="gm0160_v1__chp_1__art_1.1__ref_3">Noorderburen</tekst:IntIoRef> (voorheen bekend als Noordereilanden)</tekst:Al>
		  </tekst:Inhoud>
		</tekst:Lid>
		<tekst:Lid eId="chp_1__art_1.1__para_4" wId="gm0160_v1__chp_1__art_1.1__para_4">
		  <tekst:LidNummer>4.</tekst:LidNummer>
		  <tekst:Inhoud>
		    <tekst:Al>Het havengebied in dit omgevingsplan omvat de <tekst:IntIoRef eId="chp_1_art1.1__ref_4" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1" wId="gm0160_v1__chp_1_art1.1__ref_4">Oostelijke
                                         <tekst:VerwijderdeTekst>havens</tekst:VerwijderdeTekst><tekst:NieuweTekst>havenszijde</tekst:NieuweTekst></tekst:IntIoRef>.</tekst:Al>
		  </tekst:Inhoud>
		</tekst:Lid>
	      </tekst:Artikel>
	    </tekst:Hoofdstuk>
	    <tekst:Hoofdstuk eId="chp_2" wId="gm0160_v1__chp_2">
	      <tekst:Kop>
		<tekst:Label>Hoofdstuk</tekst:Label>
		<tekst:Nummer>2</tekst:Nummer>
		<tekst:Opschrift>Horeca</tekst:Opschrift>
	      </tekst:Kop>
	      <tekst:Artikel eId="chp_2__art_2.1" wId="gm0160_v1__chp_2__art_2.1">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>2.1</tekst:Nummer>
		  <tekst:Opschrift>Regels voor horeca</tekst:Opschrift>
		</tekst:Kop>
		<tekst:Inhoud>
		  <tekst:Al>Het starten van een horecabedrijf is toegestaan in <tekst:VerwijderdeTekst>het
                                     horecagebied</tekst:VerwijderdeTekst><tekst:NieuweTekst><tekst:IntIoRef eId="chp_2_art001__ref_1" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1" wId="gm0160_v1__chp_2_art001__ref_1">Binnenstad West</tekst:IntIoRef></tekst:NieuweTekst>, mits er voldoende
                                     parkeergelegenheid aanwezig is en er een vergunning is verleend.</tekst:Al>
		</tekst:Inhoud>
	      </tekst:Artikel>
	      <tekst:Artikel eId="chp_2__art_2.2" wId="gm0160_v1__chp_2__art_2.2">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>2.2</tekst:Nummer>
		  <tekst:Opschrift>In het openbaar nuttigen van alcohol</tekst:Opschrift>
		  <tekst:Subtitel>Het nuttigen van alcohol in het openbaar buiten de
                                     horecagebouwen</tekst:Subtitel>
		</tekst:Kop>
		<tekst:Inhoud>
		  <tekst:Al>Het is niet toegestaan om tussen 20:00u en 04:00u de daaropvolgende dag, in
                                     het openbaar alcohol te nuttigen buiten of in de directe omgeving van de
                                     horecagebouwen die zijn gevestigd in de <tekst:IntIoRef eId="chp_2_art002__ref_1" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1" wId="gm0160_v1__chp_2_art002__ref_1">Binnenstad West</tekst:IntIoRef>.</tekst:Al>
		</tekst:Inhoud>
	      </tekst:Artikel>
	    </tekst:Hoofdstuk>
	    <tekst:Hoofdstuk eId="chp_3" wId="gm0160_v1__chp_3">
	      <tekst:Kop>
		<tekst:Label>Hoofdstuk</tekst:Label>
		<tekst:Nummer>3</tekst:Nummer>
		<tekst:Opschrift>Consumentenvuurwerk</tekst:Opschrift>
	      </tekst:Kop>
	      <tekst:Artikel eId="chp_3__art_3.1" wId="gm0160_v1__chp_3__art_3.1">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>3.1</tekst:Nummer>
		  <tekst:Opschrift>Regels voor het gebruik van consumentenvuurwerk</tekst:Opschrift>
		</tekst:Kop>
		<tekst:Inhoud>
		  <tekst:Al>Het is verboden consumentenvuurwerk te gebruiken in <tekst:IntIoRef eId="chp_3__art_3.1__ref_1" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1" wId="gm0160_v1__chp_3__art_3.1__ref_1">Binnenstad West</tekst:IntIoRef>, tijdens de jaarwisseling omdat dit gevaar,
                                     schade en overlast veroorzaakt.</tekst:Al>
		  <tekst:Al wijzigactie="voegtoe">In het gebied <tekst:IntIoRef eId="chp_3__art_3.1__ref_2" ref="gm0160_v2__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1" wId="gm0160_v1__chp_3__art_3.1__ref_2">Noordereilanden</tekst:IntIoRef> geldt een algeheel verbod om vuurwerk te
                                     gebruiken.</tekst:Al>
		</tekst:Inhoud>
	      </tekst:Artikel>
	      <tekst:Artikel eId="chp_3__art_3.2" wId="gm0160_v1__chp_3__art_3.2">
		<tekst:Kop>
		  <tekst:Label>Artikel</tekst:Label>
		  <tekst:Nummer>3.2</tekst:Nummer>
		  <tekst:Opschrift><tekst:VerwijderdeTekst>Regels voor het gebruik van
                                     carbidschieten</tekst:VerwijderdeTekst><tekst:NieuweTekst>In het openbaar nuttigen van
                                         alcohol</tekst:NieuweTekst></tekst:Opschrift>
		  <tekst:Subtitel wijzigactie="voegtoe">Het nuttigen van alcohol in het openbaar buiten de
                                     horecagebouwen</tekst:Subtitel>
		</tekst:Kop>
		<tekst:Inhoud>
		  <tekst:Al wijzigactie="verwijder">Op 31 december van het jaar en tot 1 uur na de jaarwisseling is
                                     carbidschieten toegestaan in het gebied <tekst:IntIoRef eId="chp_3_art002__ref_1_inst2" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_2__ref_o_1" wId="gm0160_v1__chp_3_art002__ref_1_inst2">Zuiderburen</tekst:IntIoRef>,
                                     indien daarvoor vergunning is verleend.</tekst:Al>
		  <tekst:Al wijzigactie="voegtoe">Het is niet toegestaan om tussen 20:00u en 04:00u de
                                     daaropvolgende dag, in het openbaar alcohol te nuttigen buiten of in de directe omgeving
                                     van de horecagebouwen die zijn gevestigd in de <tekst:IntIoRef eId="chp_2_art3.2__ref_1" ref="gm0160_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1" wId="gm0160_v1__chp_2_art3.2__ref_1">Binnenstad
                                         West</tekst:IntIoRef>.</tekst:Al>
		</tekst:Inhoud>
	      </tekst:Artikel>
	    </tekst:Hoofdstuk>
	    <tekst:Hoofdstuk eId="chp_4" wId="gm0160_v1__chp_4">
