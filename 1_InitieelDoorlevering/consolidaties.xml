                <Lichaam wId="body" eId="body">
                    <Hoofdstuk wId="gm0299_v1__chp_0" eId="chp_0">
                        <Kop>
                            <Label>Hoofdstuk</Label>
                            <Nummer>0</Nummer>
                            <Opschrift>Over dit omgevingsplan</Opschrift>
                        </Kop>
                        <Artikel eId="chp_0__art_0.1" wId="gm0299_v1__chp_0__art_0.1">
                            <Kop>
                                <Label>Artikel</Label>
                                <Nummer>0.1</Nummer>
                                <Opschrift>Toepassingsgebied</Opschrift>
                            </Kop>
                            <Inhoud>
                                <Al><IntRef ref="cmp_I">Bijlage I</IntRef> bij dit omgevingsplan bevat begripsbepalingen voor de toepassing van dit omgevingsplan.</Al>
                            </Inhoud>
                        </Artikel>
                    </Hoofdstuk>
                    <Hoofdstuk eId="chp_1" wId="gm0299_v1__chp_1">
                        <Kop>
                            <Label>HOOFDSTUK</Label>
                            <Nummer>1</Nummer>
                            <Opschrift>ALGEMENE BEPALINGEN</Opschrift>
                        </Kop>
                        <Artikel eId="chp_1__art_1" wId="gm0299_v1__chp_1__art_1">
                            <Kop>
                                <Label>Artikel</Label>
                                <Nummer>1</Nummer>
                                <Opschrift>Begripsbepalingen</Opschrift>
                            </Kop>
                            <Lid eId="chp_1__art_1__para_1" wId="gm0299_v1__chp_1__art_1__para_1">
                                <LidNummer>1.</LidNummer>
                                <Inhoud>
                                    <Al>Begripsbepalingen die, op de dag van de inwerkingtreding van de Omgevingswet, zijn opgenomen in de bijlage bij de Omgevingswet en in bijlage I bij het Besluit activiteiten leefomgeving, bijlage I bij het Besluit bouwwerken leefomgeving, bijlage I bij het Besluit kwaliteit leefomgeving, bijlage I bij het Omgevingsbesluit en bijlage I bij de Omgevingsregeling, zijn van toepassing op dit omgevingsplan.</Al>
                                </Inhoud>
                            </Lid>
                            <Lid eId="chp_1__art_1__para_2" wId="gm0299_v1__chp_1__art_1__para_2">
                                <LidNummer>2.</LidNummer>
                                <Inhoud>
                                    <Al>Vanaf de datum van inwerkingtreding maakt <IntIoRef
                                        ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1"
                                        eId="chp_1_art_1__ref_1"
                                        wId="gm0299_v1__chp_1_art_1__ref_1">Noordereilanden</IntIoRef> deel uit van het ambtsgebied.</Al>
                                    <!-- vervang <Al>
                                    <Al>Vanaf de datum van inwerkingtreding maakt <IntIoRef
                                        ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1"
                                        eId="chp_1_art_1__ref_1"
                                        wId="gm0299_v1__chp_1_art_1__ref_1">Noorderburen</IntIoRef> (voorheen bekend als Noordereilanden) deel uit van het ambtsgebied.</Al>
                                    -->
                                </Inhoud>
                            </Lid>
                            <Lid eId="chp_1__art_1__para_3" wId="gm0299_v1__chp_1__art_1__para_3">
                                <LidNummer>3.</LidNummer>
                                <Inhoud>
                                    <Al>Tot de kustgebieden in dit omgevingsplan moeten worden gerekend: <IntIoRef
                                        ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_2__ref_o_1"
                                        eId="chp_1_art_1__ref_2"
                                        wId="gm0299_v1__chp_1_art_1__ref_2">Zuiderburen</IntIoRef> en <IntIoRef
                                            ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1"
                                            eId="chp_1_art1.1__ref_3"
                                            wId="gm0299_v1__chp_1_art1.1__ref_3">Noordereilanden</IntIoRef> </Al>
                                    <!-- vervang <Al>
                                    <Al>Tot de stiltegebieden in dit omgevingsplan moeten worden gerekend: <IntIoRef
                                        ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_2__ref_o_1"
                                        eId="chp_1_art_1__ref_2"
                                        wId="gm0299_v1__chp_1_art_1__ref_2">Zuiderburen</IntIoRef> (inclusief het zuidboveneiland) en <IntIoRef
                                            ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_3__ref_o_1"
                                            eId="chp_1_art1.1__ref_3"
                                            wId="gm0299_v1__chp_1_art1.1__ref_3">Noorderburen</IntIoRef> (voorheen bekend als Noordereilanden) </Al>
                                    -->
                                </Inhoud>
                            </Lid>
                            <Lid eId="chp_1__art_1__para_4" wId="gm0299_v1__chp_1__art_1__para_4">
                                <LidNummer>4.</LidNummer>
                                <Inhoud>
                                    <Al>Het havengebied in dit omgevingsplan omvat de <IntIoRef
                                        ref="gm0299_v1__cmp_I__content_o_1__list_o_1__item_o_1__ref_o_1"
                                        eId="chp_1_art1.1__ref_4" wId="gm0299_v1__chp_1_art1.1__ref_4">Oostelijke havens</IntIoRef>.</Al>
